{
     "tests": [
       {
         "name": "Basic Operations",
         "capacity": 2,
         "operations": [
           {"op": "put", "key": 1, "value": 1, "expected": null},
           {"op": "put", "key": 2, "value": 2, "expected": null},
           {"op": "get", "key": 1, "expected": 1},
           {"op": "put", "key": 3, "value": 3, "expected": null},
           {"op": "get", "key": 2, "expected": -1}
         ]
       },
       {
         "name": "Single Capacity",
         "capacity": 1,
         "operations": [
           {"op": "put", "key": 1, "value": 1, "expected": null},
           {"op": "put", "key": 2, "value": 2, "expected": null},
           {"op": "get", "key": 1, "expected": -1},
           {"op": "get", "key": 2, "expected": 2}
         ]
       },
       {
         "name": "Update Existing",
         "capacity": 2,
         "operations": [
           {"op": "put", "key": 1, "value": 1, "expected": null},
           {"op": "put", "key": 1, "value": 10, "expected": null},
           {"op": "get", "key": 1, "expected": 10}
         ]
       },
       {
         "name": "Empty Cache",
         "capacity": 2,
         "operations": [
           {"op": "get", "key": 1, "expected": -1}
         ]
       }
     ]
   }
